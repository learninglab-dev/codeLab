<!DOCTYPE html>
  <html lang="en-US">
  <head>
    <% include partials/head %>
    <link rel='stylesheet' href='/stylesheets/channel_hist_output.css'>
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <p>
      List of messages for shoot: <%= shootId %>
    </p>
    <table>
      <%
        slack.forEach(function(element){
      %>
      <tr>
        <td>
          <strong>user: </strong><%= element.user %>
        </td>
        <td>
          <strong>username: </strong><%= element.username %>
        </td>
        <td>
          <strong>text: </strong><%= element.text %>
        </td>
      </tr>
      <%
        });
      %>
    </table>
  </body>
<% include partials/foot %>
